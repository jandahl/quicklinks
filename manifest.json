{
    "manifest_version": 2,
    "name": "Nianet Quicklinks, Dashboard edition",
    "description": "Fuckin' A' made a webapp outta this shit",
    "version": "1.3",
    "app": {
        "launch": {
            "local_path": "quicklinks.htm"
        }
    },
